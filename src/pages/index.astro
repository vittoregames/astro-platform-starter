<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <title>PDFs</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        margin: 20px;
      }
      .grid {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 20px;
      }
      iframe {
        width: 100%;
        height: 500px;
        border: 1px solid #ccc;
      }
    </style>
  </head>

  <body>
    <h1>Documentos PDF</h1>

    <div class="grid">
      <iframe src="/pdfs/TFI CompletoV5.docx"></iframe>
      <iframe src="/pdfs/doc2.pdf"></iframe>
      <iframe src="/pdfs/doc3.pdf"></iframe>
      <iframe src="/pdfs/doc4.pdf"></iframe>
    </div>
  </body>
</html>
